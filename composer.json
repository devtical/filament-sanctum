{
    "name": "devtical/filament-sanctum",
    "description": "Laravel Sanctum integration for Filament 4",
    "version": "1.0.0",
    "keywords": [
        "devtical",
        "sanctum",
        "laravel",
        "filament"
    ],
    "homepage": "https://github.com/devtical/filament-sanctum",
    "license": "MIT",
    "authors": [
        {
            "name": "W Kristianto",
            "email": "w.kristories@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/filament": "^4.0",
        "illuminate/contracts": "12.*",
        "laravel/sanctum": "^4.0",
        "orchestra/testbench": "10.*",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Devtical\\Sanctum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devtical\\Sanctum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --no-coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtical\\Sanctum\\SanctumServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
